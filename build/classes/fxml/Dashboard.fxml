<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="studentsystem.controllers.DashboardController"
            prefWidth="600" prefHeight="400">

    <children>
        <Label layoutX="250" layoutY="30" text="Dashboard" style="-fx-font-size: 24px;"/>
        <Button fx:id="logoutButton" layoutX="500" layoutY="20" text="Logout" onAction="#handleLogout"/>
        <Button layoutX="200" layoutY="100" prefWidth="200" text="Student Management" onAction="#goToStudentManagement"/>
    </children>
</AnchorPane>
